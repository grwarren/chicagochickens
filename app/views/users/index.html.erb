<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Current Users</h3>
  </div>
  <div class="panel-body">
    <% if !@users.empty? %>
      <table class="table table-bordered">
        <tr>
          <td>
            User Id
          </td>
          <td>
            Name
          </td>
        </tr>

        <% @users.each do |user| %>
          <tr>
            <td> <%= user.user_id %></td>
            <td> <%= user.name  %></td>
          </tr>
        <% end %>
      </table>
    <% else %>
      <p>
        No User found. Please add some users.
        Thank you!
      </p>
  <% end %>
  </div>
</div>
