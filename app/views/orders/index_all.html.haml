.panel.panel-default
  .panel-heading
    %h3.panel-title Current Orders
  .panel-body
    - if !@orders.empty?
      %table.table.table-bordered
        %tr
          %th Product
          %th User
          %th= @orders.last.delivery_date
        - @orders.each do |order|
          %tr
            %td= order.product.name
            %td= order.user.name
            %td= order.quantity
    - else
      %p
        No Orders found